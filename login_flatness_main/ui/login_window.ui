QPushButton#register_pushButton,
QPushButton#mysql_pushButton {
    background-color: rgba(0, 0, 0, 0);
    border: none;
    border-bottom: 1px solid rgb(179, 179, 179);
    color: rgba(0, 0, 0, 240);
}

QPushButton#github_pushButton,
QPushButton#phone_pushButton,
QPushButton#email_pushButton {
    background-color: rgba(0, 0, 0, 0);
    color: rgba(85, 98, 112, 255);
}

QPushButton#github_pushButton:hover,
QPushButton#phone_pushButton:hover,
QPushButton#email_pushButton:hover {
    color: rgba(131, 96, 53, 255);
}

QPushButton#github_pushButton:pressed,
QPushButton#phone_pushButton:pressed,
QPushButton#email_pushButton:pressed {
    padding-left: 5px;
    padding-top: 5px;
    color: rgba(91, 88, 53, 255);
}

QPushButton#close_pushButton{
 background-color: #ce5137;
 border-radius:10px;
}

QPushButton#close_pushButton:hover{
 background-size: cover;
 background-image: url(:/icons/icons/close.svg);
}

QPushButton#min_pushButton{
