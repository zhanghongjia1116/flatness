}

QPushButton#login_pushButton:hover {
    background-color: qlineargradient(spread: pad, x1: 0, y1: 0.505682, x2: 1, y2: 0.477, stop: 0 rgba(150, 123, 1111, 219), stop: 1 rgba(85, 81, 84, 226));
}

QPushButton#entry_pushButton {
border: none; /* 这里设置了无边框 */
}


